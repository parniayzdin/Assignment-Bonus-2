Algorithm TranslateModelToJava(xmlFile):
  doc <-- parseXML(xmlFile)

  //Rectangles with name text 
  things <-- empty map //(id --> className)
  for each diagramElement c in doc:
    if c.vertex == "1" AND text(c.value) is not empty AND isRectangle(c):
      things[c.id] <-- cleanText(c.value)

  //Arrows imply relationships (association or specialization)
  edges <-- empty list
  for each diagramElement e in doc:
    if e.edge == "1":
      src <-- e.source
      target <-- e.target
      label <-- findLabelNearArrow(e, doc)     
      kind <-- classifyArrow(e.style)         
      edges.add((src, target, kind, label))

  //Validate Well-formedness rule 
  for each (src, target, kind, label) in edges:
    assert src in things AND target in things   

    if kind == ASSOCIATION:
	    assert label is not empty
      assert label matches "name (1)" OR "name (N)"   //every association has exactly one name and one cardinality

	//Specialization must not be circular
	parent <-- empty map //(childId --> parentId)
	for each (src, target, kind, label) in edges:
		if kind == SPECIALIZATION:
				parent[src] <-- target
	for each nodeId in things:
		current <-- nodeId
		while current in parent:
			current <-- parent[current]
			if current == nodeId:
				error "Specialization must not be circular"
				
  //Generate Java code using translational semantics
  for each thing in things:
    className <-- things[thing]
    superClass <-- none
    fields <-- empty list

    //find superclass (if any)
    for each (src, target, kind, label) in edges:
      if kind == SPECIALIZATION AND src == thing:
        superClass <-- things[target]

    //create fields for associations
    for each (src, target, kind, label) in edges:
      if kind == ASSOCIATION AND src == thing:
        (relationName, mult) <-- parseLabel(label)
        targetClass <-- things[target]

        if mult == "1":
          fieldType <-- targetClass
        else:
          fieldType <-- "List<" + targetClass + ">"

        fieldName <-- chooseField(relationName, targetClass, mult)
        fields.add((fieldType, fieldName))
